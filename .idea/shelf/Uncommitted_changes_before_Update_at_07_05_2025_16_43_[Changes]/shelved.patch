<+>{% extends 'base.html.twig' %}\r\n\r\n{% block body %}\r\n    <link href=\"{{ asset('css/profile.css') }}\" rel=\"stylesheet\">\r\n    <div class=\"profile-container\">\r\n        <div class=\"profile-header\">\r\n            <img src=\"{{ user.profile_medium }}\" alt=\"Profile Picture\" class=\"profile-image\">\r\n            <div class=\"profile-info\">\r\n                <h1>{{ user.firstname }} {{ user.lastname }}</h1>\r\n                <p><strong>Username:</strong> {{ user.username }}</p>\r\n                <p><strong>Location:</strong> {{ user.city }}, {{ user.state }}, {{ user.country }}</p>\r\n                <p><strong>Sex:</strong> {{ user.sex == 'M' ? 'Male' : 'Female' }}</p>\r\n                <p><strong>Premium:</strong> {{ user.premium ? 'Yes' : 'No' }}</p>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"profile-stats\">\r\n            <h2>Profile Stats</h2>\r\n            <p><strong>Follower Count:</strong> {{ user.follower }}</p>\r\n            <p><strong>Friend Count:</strong> {{ user.friend }}</p>\r\n        </div>\r\n    </div>\r\n{% endblock %}\r\n
